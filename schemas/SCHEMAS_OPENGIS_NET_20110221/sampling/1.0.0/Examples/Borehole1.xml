			<gml:pos>-30.7115 134.2055 272.</gml:pos>
			<gml:pos>-30.7116 134.2054 271.</gml:pos>
			<gml:pos>-30.7117 134.2053 270.</gml:pos>
		</gml:LineString>
	</sa:shape>
	<sa:length uom="m">55.</sa:length>
</sa:SamplingCurve>
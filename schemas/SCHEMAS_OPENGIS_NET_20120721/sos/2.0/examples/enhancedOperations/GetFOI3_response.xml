					<sams:shape>
						<gml:Point gml:id="st1p">
							<gml:pos srsName="http://www.opengis.net/def/crs/EPSG/0/4326">50.7167 7.76667</gml:pos>
						</gml:Point>
					</sams:shape>
				</sams:SF_SpatialSamplingFeature>
			</sos:featureMember>
		</sos:GetFeatureOfInterestResponse>
	</soap12:Body>
</soap12:Envelope>

                      http://www.opengis.net/gml
                      http://schemas.opengis.net/gml/3.1.1/base/gml.xsd">
  <Query typeName="tds:RoadGeocurve" handle="Q01">
    <ogc:Filter>
      <ogc:BBOX>
        <ogc:PropertyName>geometry</ogc:PropertyName>
        <gml:Envelope srsName="urn:ogc:def:crs:EPSG::4326">
           <gml:lowerCorner>17.4906 -75.0396</gml:lowerCorner>
           <gml:upperCorner>20.0306 -70.8064</gml:upperCorner>
        </gml:Envelope>
      </ogc:BBOX>
    </ogc:Filter>
  </Query>
</GetFeature>

<?xml version='1.0' encoding='UTF-8'?>
<wps:ProcessDescriptions xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" service="WPS" version="1.0.0" xml:lang="en-US" xsi:schemaLocation="wps http://schemas.opengis.net/wps/1.0.0/wpsAll.xsd">
  <ProcessDescription wps:processVersion="1.0.0">
    <ows:Identifier>TC10:identity:complex:binary</ows:Identifier>
    <ows:Title>Test Case 10: Binary complex data identity.</ows:Title>
    <ows:Abstract> Test identity process (the outputs are copies of the inputs)
    demonstrating various features of the binary complex data inputs and outputs.
    </ows:Abstract>
    <ows:Metadata xlink:title="test-metadata" xlink:href="http://www.metadata.com/test-metadata"/>
    <wps:Profile>test_profile</wps:Profile>
    <DataInputs>
      <Input minOccurs="1" maxOccurs="1">
        <ows:Identifier>TC10:input</ows:Identifier>
        <ows:Title>Test case #10: Complex input #00</ows:Title>
        <ows:Abstract>Binary complex data input.</ows:Abstract>
        <ComplexData>
          <Default>
            <Format>
              <MimeType>application/octet-stream</MimeType>
              <Encoding>base64</Encoding>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>application/octet-stream</MimeType>
              <Encoding>base64</Encoding>
            </Format>
            <Format>
              <MimeType>application/octet-stream</MimeType>
            </Format>
          </Supported>
        </ComplexData>
      </Input>
    </DataInputs>
    <ProcessOutputs>
      <Output>
        <ows:Identifier>TC10:output</ows:Identifier>
        <ows:Title>Test case #10: Binary complex output #00</ows:Title>
        <ows:Abstract>Binary complex data output (copy of the input).</ows:Abstract>
        <ComplexOutput>
          <Default>
            <Format>
              <MimeType>application/octet-stream</MimeType>
              <Encoding>base64</Encoding>
            </Format>
          </Default>
          <Supported>
            <Format>
              <MimeType>application/octet-stream</MimeType>
              <Encoding>base64</Encoding>
            </Format>
            <Format>
              <MimeType>application/octet-stream</MimeType>
            </Format>
          </Supported>
        </ComplexOutput>
      </Output>
    </ProcessOutputs>
  </ProcessDescription>
</wps:ProcessDescriptions>
